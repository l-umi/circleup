{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable", "ES2015.Promise", "ES2015.Iterable"] // 필요한 라이브러리 추가
  },
  "include": ["src"]
}
